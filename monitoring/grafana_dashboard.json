{
  "dashboard": {
    "id": null,
    "uid": "mia-system-overview",
    "title": "MIA System Overview",
    "tags": ["mia", "trading", "monitoring"],
    "timezone": "Asia/Shanghai",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "type": "stat",
        "title": "Soldier Mode",
        "description": "Current Soldier execution mode (Local AMD GPU or Cloud API)",
        "targets": [
          {
            "expr": "mia_soldier_mode",
            "refId": "A",
            "legendFormat": "Mode"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "Local",
                    "color": "green"
                  },
                  "1": {
                    "text": "Cloud",
                    "color": "yellow"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "yellow"
                }
              ]
            },
            "unit": "none"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 0
        },
        "type": "timeseries",
        "title": "Soldier Latency P99",
        "description": "99th percentile decision latency (target: <150ms)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(mia_soldier_latency_seconds_bucket[5m])) * 1000",
            "refId": "A",
            "legendFormat": "{{mode}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 150,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "type": "timeseries",
        "title": "Trade Volume (5m)",
        "description": "Trading volume per 5 minutes by strategy",
        "targets": [
          {
            "expr": "rate(mia_trades_total[5m]) * 300",
            "refId": "A",
            "legendFormat": "{{strategy}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "bars",
              "lineWidth": 1,
              "fillOpacity": 80
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 4,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 4
        },
        "type": "stat",
        "title": "Daily PnL",
        "description": "Daily profit and loss",
        "targets": [
          {
            "expr": "mia_portfolio_pnl{period='daily'}",
            "refId": "A",
            "legendFormat": "Daily PnL"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "CNY",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "red"
                },
                {
                  "value": 0,
                  "color": "yellow"
                },
                {
                  "value": 0.01,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 5,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "timeseries",
        "title": "Portfolio Value",
        "description": "Total portfolio value over time",
        "targets": [
          {
            "expr": "mia_portfolio_value",
            "refId": "A",
            "legendFormat": "Total Value"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "unit": "CNY",
            "decimals": 2,
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 6,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "timeseries",
        "title": "GPU Memory Usage",
        "description": "GPU memory utilization percentage",
        "targets": [
          {
            "expr": "mia_gpu_memory_used_bytes / mia_gpu_memory_total_bytes * 100",
            "refId": "A",
            "legendFormat": "GPU Memory %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 30,
              "showPoints": "never"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 80,
                  "color": "yellow"
                },
                {
                  "value": 90,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 7,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "type": "timeseries",
        "title": "System Resources",
        "description": "CPU, Memory, and Disk usage",
        "targets": [
          {
            "expr": "mia_system_cpu_percent",
            "refId": "A",
            "legendFormat": "CPU %"
          },
          {
            "expr": "mia_system_memory_percent",
            "refId": "B",
            "legendFormat": "Memory %"
          },
          {
            "expr": "mia_system_disk_percent{drive='D:'}",
            "refId": "C",
            "legendFormat": "Disk D: %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 80,
                  "color": "yellow"
                },
                {
                  "value": 90,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "id": 8,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "type": "timeseries",
        "title": "Redis Latency",
        "description": "Average Redis operation latency by operation type",
        "targets": [
          {
            "expr": "rate(mia_redis_latency_seconds_sum[5m]) / rate(mia_redis_latency_seconds_count[5m]) * 1000",
            "refId": "A",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "unit": "ms",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 10,
                  "color": "yellow"
                },
                {
                  "value": 50,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "blue",
          "tags": ["deployment"]
        },
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "red",
          "tags": ["alert"]
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(mia_system_cpu_percent, instance)",
          "refresh": 1,
          "includeAll": false,
          "multi": false
        }
      ]
    }
  },
  "overwrite": true,
  "folderId": 0,
  "folderUid": null
}
